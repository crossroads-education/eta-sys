{
  "compilerOptions": {
    "target": "es5",
    "module": "amd",
    "moduleResolution": "classic",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "preserveConstEnums": true
  },
  "compileOnSave": true,
  "buildOnSave": false,
  "exclude": ["node_modules", "typings"],
  "atom": {
    "rewriteTsconfig": false
  }
}
